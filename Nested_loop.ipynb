      "source": [
        "n = [3, 10, 4, 6, 8, 9, 7, 2, 1, 5]\n",
        "for i in range(1, len(n)):\n",
        "  a = n[i]\n",
        "  j = i - 1\n",
        "  while j >= 0 and n[j] > a:\n",
        "    n[j + 1] = n[j];\n",
        "    j -= 1\n",
        "    n[j + 1] = a\n",
        "\n",
        "  print(n)"
      ]
    }
